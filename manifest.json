{
   "name": "Send To WhatsApp Web",
   "description" : "Send messages to ANY phone number appearing on a webpage",
   "version": "1.0",
   "manifest_version": 2,
   "permissions": [
     "activeTab",
     "tabs",
     "https://web.whatsapp.com/"
   ],

   "content_scripts": [
     {
       "matches": ["<all_urls>"],
       "js": [
         "lib/require.js",
         "lib/libphonenumber-js.min.js",
         "contentScript.js"
       ]
     }
   ]
 }
